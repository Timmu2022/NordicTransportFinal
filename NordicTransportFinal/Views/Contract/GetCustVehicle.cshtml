@model IEnumerable<NordicTransportFinal.Data.Models.Domain.CustVehicle>

@{
    ViewBag.Title = "GetCustVehicle";
}

<style>
    .stylecontainer {
        margin: 2%;
        margin-top: 5%;
    }
</style>

<h2 class="stylecontainer">Customer Vehicle</h2>

<p>

</p>
<table class="table stylecontainer">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TravelDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DistanceTravelled)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TravelDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DistanceTravelled)
        </td>

    </tr>
}

</table>
